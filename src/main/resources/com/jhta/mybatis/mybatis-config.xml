		<mapper resource="com/jhta/mybatis/PetSitterMapper.xml" />
		<mapper resource="com/jhta/mybatis/FilterTypeListMapper.xml" />
		<mapper resource="com/jhta/mybatis/PetSitterImageMapper.xml" />
	 	<mapper resource="com/jhta/mybatis/PetsitterOptionMapper.xml"/>
   		<mapper resource="com/jhta/mybatis/PetsitterPriceMapper.xml"/>
	   	<mapper resource="com/jhta/mybatis/PpetInfoMapper.xml"/>
	   <!-- Member -->
		<mapper resource="com/jhta/mybatis/memberMApper.xml" />
		<!--  Shop -->
		<mapper resource="com/jhta/mybatis/ShopAdminMapper.xml" />
		<mapper resource="com/jhta/mybatis/ShopMapper.xml" />	
	</mappers>

