<configuration>

	<typeAliases>

		<!-- Petsitter -->
		<typeAlias type="com.jhta.project.vo.FilterTypeListVo" alias="FilterTypeList" />
		<typeAlias type="com.jhta.project.vo.PetSitterImageVo" alias="petsitterimage" />
		<typeAlias type="com.jhta.project.vo.PetSitterJoinFilterVo" alias="petsitterjoinfilter" />
		<typeAlias type="com.jhta.project.vo.PetsitterOptionVo" alias="petoption"/>
