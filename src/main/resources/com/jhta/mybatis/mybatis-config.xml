		<typeAlias type="com.jhta.project.vo.PetSitterVo" alias="petsitter" />
  		
		<!--  Member -->
		<typeAlias alias="member" type="com.jhta.project.vo.memberVO" />
		<!--  Shop -->
		<typeAlias type="com.jhta.project.vo.ShopClassVo" alias="shopclassvo" />
		<typeAlias type="com.jhta.project.vo.ShopFieldVo" alias="shopfieldvo" />
